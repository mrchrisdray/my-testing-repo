        "gitpython",
        "pytest"
    ],
    "cSpell.words": [
        "addopts",
        "autouse",
        "caplog",
        "docparams",
        "docstrings",
        "docstyle",
        "ELIF",
        "fstring",
        "indentless",
        "levelname",
        "mypy",
        "pygh",
        "pylint",
        "pylintrc",
        "PYTHONPATH",
        "pyyaml",
        "rcfile",
        "subteam",
        "subteams",
        "testpaths",
        "tmpdirname",
